<div>
  <i class="main-icon fa fa-<%= mainIcon %>"></i> <label><%= title %></label>
</div>
<i class="fa fa-close"></i>
<ul>
  <% menuItems.forEach(function(menuItem) { %>
    <li class="<%= menuItem.func %>-button">
      <i class="fa fa-<%= menuItem.glyph %>"></i><label><%= menuItem.name %></label>
    </li>
  <% }) %>
</ul>
