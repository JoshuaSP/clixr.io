<nav class="main-navbar clearfix">
  <div class="logo">
    clixr.io
  </div>
  <div class="page-select">
    <span class="page-identifier">Page:</span> <span class="current-page-name"><%= currentPage.escape('title') %></span>
    <ul>
      <% this.model.pages().forEach(function(page, index) { %>
        <li><%= page.escape('title') %></li>
      <% }) %>
    </ul>

  </div>
</nav>

<div class="function-buttons">
</div>

<div class="floating-menus">
</div>

<div class="user-page-container">

  <div class="user-site include-user-fonts">
    <img class="user-background-image" src="<%= site.escape('background_url') %>" alt=""
      style="z-index:-1; position: fixed; width: 100vw; height: 100vh;"/>

    <div class="user-page">
      <div class="user-page-gridlines">
        <div class="user-page-elements current" data-page-ord=0>
        </div>
        <% for (var i = 1; i < this.model.pages().length; i++) { %>
          <div class="user-page-elements" data-page-ord=<%= i %>>
          </div>
        <% } %>
        <div class="user-site-elements">
        </div>
      </div>
    </div>
  </div>
</div>

<div class="modals">

</div>
